package string_concepts.mystring1;

import java.util.stream.Collectors;
import java.util.stream.Stream;

//find duplicate words and their number of occurrences of them
public class DuplicateWordsAndOccurrenece {

	public static void main(String[] args) {
		String s = "Bread butter and bread";
		Stream.of(s.toLowerCase().split(" ")).collect(Collectors.groupingBy(String::valueOf)).entrySet().forEach(System.out::println);;
	}
}
