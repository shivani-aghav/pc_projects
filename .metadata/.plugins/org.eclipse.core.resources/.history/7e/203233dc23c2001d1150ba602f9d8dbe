package string_concepts.mystring1;

import java.util.stream.Collectors;
import java.util.stream.Stream;

public class MostRepetitive {
	public static void main(String[] args) {
		String s = "javaconceptoftheday";
		char[] c = s.toCharArray();

       Stream.of(s.split("")).collect(Collectors.groupingBy(t->t)).entrySet().forEach(o->{
    	   if (o.getValue().size() > 1) {
				System.out.println( o.getKey()+" : "+ o.getValue().size());
			}
       });
         
	}
}
