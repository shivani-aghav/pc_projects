package concepts;

public class JavaLoopsII {

	public static void main(String[] args) {
		int a = 5;
		int b = 3;
		int n = 5;
		int x = 1;// 2//4
		int m = a + 1 * b;
		// a+1 * b * b+x*x *b+x*x*x *b+x*x*x*x *b

		for (int i = 0; i < n; i++) {
			System.out.print(m + " ");
			x = x * 2;
			m = m + x * b;
		}
	}
}
