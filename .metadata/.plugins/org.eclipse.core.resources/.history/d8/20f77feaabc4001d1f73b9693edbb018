package concepts;

import java.util.Arrays;
import java.util.stream.IntStream;

public class CountingValleys {

	public static void main(String[] args) {
		int n=10;
		Arrays.asList(1,2,3,4,5).stream().reduce((t, u) -> t*u ).ifPresent(System.out::println);
		IntStream.range(0, n).reduce((l, r) -> l*r).ifPresent(System.out::println);
	}
}
