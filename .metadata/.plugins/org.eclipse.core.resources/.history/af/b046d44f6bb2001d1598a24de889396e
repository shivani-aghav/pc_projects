package basicprograms.revision;

import java.util.Arrays;
import java.util.List;

public class ArrayProgram {

	public static void main(String[] args) {
		List<Employee> list = Arrays.asList(
				new Employee(1, "Carol", 12000, "M", Arrays.asList("KGF", "Annabelle")),
				new Employee(2, "Linda", 8000, "F", Arrays.asList("Pachadlela", "Conjuring")),
				new Employee(3, "Neena", 34000, "M", Arrays.asList("Vedha", "Vikram")),
				new Employee(4, "Jenny", 20000, "F", Arrays.asList("IT", "Nun")));

	}

}
