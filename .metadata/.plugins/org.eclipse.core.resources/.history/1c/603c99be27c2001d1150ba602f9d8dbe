package string_concepts.mystring1;

import java.util.List;
import java.util.Map;
import java.util.stream.Collectors;
import java.util.stream.Stream;

public class MostRepetitive {
	public static void main(String[] args) {
		String s = "javaconceptoftheday";
		char[] c = s.toCharArray();

       
		Stream.of(s.split("")).collect(Collectors.groupingBy(t->t)).entrySet().stream()
		.sorted((o1, o2) ->-(o1.getValue().size()-o2.getValue().size()))
		.findFirst().ifPresent(System.out::println);
		
//		cs.entrySet().stream().map(t -> t.getValue().size()).collect(Collectors.summarizingInt(Integer::intValue)).getMax(); 
		
//		.forEach(t->{
//			System.out.println(t.getKey()+" --"+t.getValue().size())
//		});;
         
	}
}
