package string_concepts.mystring1;

import java.util.List;
import java.util.Map.Entry;
import java.util.Set;
import java.util.stream.Collectors;
import java.util.stream.Stream;

public class MostRepetitive {
	public static void main(String[] args) {
		String s = "javaconceptoftheday";
		char[] c = s.toCharArray();

//       filter(e->e.getValue()).collect(Collectors.summarizingInt(Integer::intValue))
//      ;
       
       
		Set<Entry<String, List<String>>> set = Stream.of(s.split("")).collect(Collectors.groupingBy(t->t)).entrySet();
		System.out.println(set);
         
	}
}
