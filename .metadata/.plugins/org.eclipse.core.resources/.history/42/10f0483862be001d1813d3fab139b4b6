public class Multidimensional {

	public static void main(String[] args) {
		int[] a = { 1, 2, 3 };
		int[] b = { 4, 5, 6 };
		int[] c = { 7, 8, 9 };
		int[][] d = { a, b, c };
		
		for (int i = 0; i < d.length; i++) {
			for (int j = 0; j < d[i].length; j++) {
				System.out.print(d[i][j]+"\t");
			}
			System.out.println();
		}
	}
}
