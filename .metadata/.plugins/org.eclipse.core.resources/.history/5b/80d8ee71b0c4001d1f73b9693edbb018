package concepts;

import java.util.Scanner;

public class JavaLoopsII {

	public static void main(String[] args) {
		
		 Scanner in = new Scanner(System.in);
	        int t=in.nextInt();
	        for(int i=0;i<t;i++){
	            int a = in.nextInt();
	            int b = in.nextInt();
	            int n = in.nextInt();
	        }
		int a = 5;
		int b = 3;
		int n = 5;
		int x = 1;// 2//4
		int m = a + 1 * b;
		// a+1 * b * b+x*x *b+x*x*x *b+x*x*x*x *b

		for (int i = 0; i < n; i++) {
			System.out.print(m + " ");
			x = x * 2;
			m = m + x * b;
		}
	}
}
