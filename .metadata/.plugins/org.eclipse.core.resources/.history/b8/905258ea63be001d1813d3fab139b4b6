
public class BirthdayCandleArrayProgram {

	public static void main(String[] args) {
		int[]a= {3,1,2,3};
		int big=0;
		int count=0;
		
		for (int i = 0; i < a.length; i++) {
			for (int j = i+1; j < a.length; j++) {
				if (a[i]==a[j]) {
					big=a[i];
				}
			}
		}
		for (int i = 0; i < a.length; i++) {
			if (big==a[i]) {
				count++;
			}
		}
		System.out.println(count);
	}
}
