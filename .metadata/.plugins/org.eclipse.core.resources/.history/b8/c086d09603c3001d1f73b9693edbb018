package string_concepts.mystring1;

import java.util.List;
import java.util.Map;
import java.util.Map.Entry;
import java.util.Set;
import java.util.stream.Collectors;
import java.util.stream.Stream;

public class SherlockAndValidString {

	public static void main(String[] args) {
		
		String s="abcdefghhgfedecba";
		
		Map<String, List<String>> map = Stream.of(s.split("")).collect(Collectors.groupingBy(t->t));
		Set<Entry<String, List<String>>>  set = map.entrySet();
		set.stream().filter(t -> t.getValue().size()!=t.getValue().size()).collect(Collectors.toList()).forEach(System.out::println);
	}
}
