package concepts;

import java.util.Arrays;

public class CountingValleys {

	public static void main(String[] args) {
		String s = "UDDDUDUU";
		String[] spl = s.split("");
		System.out.println(Arrays.toString(spl));
		for (int i = 0; i < spl.length; i++) {
            System.out.println(spl[i]+'\b');
		}
	}
}
