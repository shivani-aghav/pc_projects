package concepts;

import java.util.Scanner;

public class JavaLoopsII {

	public static void main(String[] args) {

		Scanner in = new Scanner(System.in);
		int a = 0;
		int b = 0;
		int n = 0;
		System.out.println("enter values");
		int t = in.nextInt();
		for (int i = 0; i < t; i++) {
			a = in.nextInt();
			b = in.nextInt();
			n = in.nextInt();
			
			int x = 1;
			int m = a + 1 * b;

			for (int j = 0; i < n; i++) {
				System.out.print(m + " ");
				x = x * 2;
				m = m + x * b;
			}
		}

		
	}
}
